package controller;

import javax.persistence.*;

/**
 * Maps the Registration Request to the MySQL Database table named "users_db"
 *
 */
//@Entity
//@Table(name = "users_db")
public class RegReq
{
    private String firstName;
    private String lastName;
    private String password;

    /**
     * Getter for user's username
     * @return Returns the users username.
     */
    public String getUsername()
    {
        return username;
    }

    /**
     * Setter for user's username
     * @param username
     */
    public void setUsername(String username)
    {
        this.username = username;
    }

    /**
     * Getter for users first name
     * @return Returns the user's first name
     */
    public String getFirstName()
    {
        return firstName;
    }

    /**
     * Setter for user's first name
     * @param firstName Returns the user's first name
     */
    public void setFirstName(String firstName)
    {
        this.firstName = firstName;
    }

    /**
     * Getter for the user's last name.
     * @return The user's last name.
     */
    public String getLastName()
    {
        return lastName;
    }

    /**
     * Setter for the user's last name.
     * @param lastName The user's last name.
     */
    public void setLastName(String lastName)
    {
        this.lastName = lastName;
    }

    /**
     * Getter for the user's password.
     * @return The user's password.
     */
    public String getPassword()
    {
        return password;
    }

    /**
     * Setter for the user's password.
     * @param password The user's password.
     */
    public void setPassword(String password)
    {
        this.password = password;
    }
}